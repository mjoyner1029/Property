{
  "name": "asset-anchor-frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.9",
    "@mui/x-date-pickers": "^6.18.2",
    "@sentry/react": "^7.101.0",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "chalk": "^4.1.2",
    "chrome-launcher": "^0.15.2",
    "framer-motion": "^12.23.12",
    "lighthouse": "^11.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "NODE_ENV=production node start.js",
    "build": "react-scripts build",
    "build:ci": "CI=true GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --watchAll=false --coverage --coverageReporters=text-summary,lcov",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:routes": "react-scripts test routes.spec.jsx guards.spec.jsx --watchAll=false",
    "test:a11y": "node scripts/run-a11y-tests.js",
    "list-routes": "node scripts/verifyRoutes.mjs",
    "verify-routes": "node scripts/verifyRoutes.mjs",
    "eject": "react-scripts eject",
    "lh:ci": "node scripts/run-lighthouse.js",
    "lighthouse": "node scripts/run-lighthouse.js",
    "deploy:cdn": "bash scripts/deploy_cdn.sh",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json}\"",
    "preview": "NODE_ENV=production node start.js",
    "check-deps": "npm audit --production",
    "generate:env": "node scripts/generate-env-example.js",
    "validate:env": "node scripts/validate-env.js",
    "prepare:release": "npm run lint && npm run test:ci && npm run build:prod && npm run test:a11y"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "statements": 70
      }
    },
    "moduleNameMapper": {
      "@heroicons/(.*)$": "<rootDir>/src/test-utils/heroicons-stub.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/test-utils/style-mock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5050"
}
