{
  "name": "asset-anchor-frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "^6.0.0-dev.240424162023-9968b4889d",
    "@mui/material": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.1",
    "@sentry/react": "^10.10.0",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "msw": "^2.11.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^12.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^30.1.2",
    "chalk": "^5.6.0",
    "chrome-launcher": "^1.2.0",
    "compression-webpack-plugin": "^11.1.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.6.8",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lighthouse": "^12.8.2",
    "prettier": "^3.6.2",
    "react-app-rewired": "^2.2.1",
    "readline-sync": "^1.4.10",
    "source-map-explorer": "^2.5.3",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "NODE_ENV=production node start.js",
    "dev": "REACT_APP_DEV_MODE=true BROWSER=none react-scripts start",
    "dev:pages": "REACT_APP_ENTRY=dev BROWSER=none react-scripts start",
    "demo:start": "node scripts/generate-msw-worker.js && REACT_APP_DEMO_MODE=1 BROWSER=none react-scripts start",
    "demo:reset": "node scripts/reset-demo-data.js",
    "build": "react-scripts build",
    "build:ci": "CI=true GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "GENERATE_SOURCEMAP=true npm run build && npm run analyze",
    "build:optimize": "GENERATE_SOURCEMAP=false REACT_APP_OPTIMIZE=true npm run build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:chunks": "webpack-bundle-analyzer build/bundle-stats.json",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 jest",
    "test:ci": "NODE_OPTIONS=--max-old-space-size=4096 jest --ci --runInBand --coverage",
    "test:changed": "NODE_OPTIONS=--max-old-space-size=4096 jest -o",
    "test:flaky": "NODE_OPTIONS=--max-old-space-size=4096 jest --watchAll=false && jest --watchAll=false && jest --watchAll=false",
    "test:routes": "react-scripts test routes.spec.jsx guards.spec.jsx --watchAll=false",
    "test:a11y": "node scripts/run-a11y-tests.js",
    "list-routes": "node scripts/verifyRoutes.mjs",
    "verify-routes": "node scripts/verifyRoutes.mjs",
    "eject": "react-scripts eject",
    "lh:ci": "node scripts/run-lighthouse.js",
    "lighthouse": "node scripts/run-lighthouse.js",
    "deploy:cdn": "bash scripts/deploy_cdn.sh",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,jsx}\"",
    "lint:tests": "eslint \"src/__tests__/**/*.{js,jsx}\"",
    "lint:tests:fix": "eslint --fix \"src/__tests__/**/*.{js,jsx}\" || true",
    "lint:unused": "eslint --fix --rule 'no-unused-vars: error' \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json}\"",
    "preview": "NODE_ENV=production node start.js",
    "check-deps": "npm audit --production",
    "check:circular": "madge --circular --extensions js,jsx src/",
    "check:duplicate": "jscpd src/",
    "generate:env": "node scripts/generate-env-example.js",
    "validate:env": "node scripts/validate-env.js",
    "typecheck": "tsc --noEmit",
    "prepare:release": "npm run lint && npm run test:ci && npm run build:optimize && npm run test:a11y"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5050",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
