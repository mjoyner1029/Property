{
  "name": "asset-anchor-frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "^6.0.0-dev.240424162023-9968b4889d",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.2",
    "axios": "^1.5.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "chalk": "^5.6.0",
    "framer-motion": "^12.23.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:ci": "CI=true react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --watchAll=false --coverage --coverageReporters=text-summary",
    "test:routes": "react-scripts test routes.spec.jsx guards.spec.jsx --watchAll=false",
    "list-routes": "node scripts/verifyRoutes.mjs",
    "verify-routes": "node scripts/verifyRoutes.mjs",
    "eject": "react-scripts eject",
    "lh:ci": "node scripts/lh-check.mjs",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,css,md,json}\"",
    "preview": "npx serve -s build -l 3000"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "statements": 70
      }
    },
    "moduleNameMapper": {
      "@heroicons/(.*)$": "<rootDir>/src/test-utils/heroicons-stub.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/test-utils/style-mock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5050"
}
