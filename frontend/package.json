{
  "name": "asset-anchor-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.9",
    "@mui/x-date-pickers": "^6.18.2",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "chalk": "^5.5.0",
    "framer-motion": "^12.23.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --watchAll=false --coverage --coverageReporters=text-summary",
    "test:routes": "react-scripts test routes.spec.jsx guards.spec.jsx --watchAll=false",
    "list-routes": "node scripts/verifyRoutes.mjs",
    "verify-routes": "node scripts/verifyRoutes.mjs",
    "eject": "react-scripts eject",
    "lh:ci": "node scripts/lh-check.mjs"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "statements": 70,
        "functions": 70
      }
    },
    "moduleNameMapper": {
      "@heroicons/(.*)$": "<rootDir>/src/test-utils/heroicons-stub.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/test-utils/style-mock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5050"
}
